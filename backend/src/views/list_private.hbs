<table id="example" class="table table-striped table-hover" style="width:100%">
        <thead>
            <tr>
                <th>id</th>
                <th>Nombre y apellidos</th>
                <th>cédula</th>
                <th>Teléfono</th>
                <th>correo</th>
                <th> Dirección </th>
                <th>Instrumento</th>
            </tr>
        </thead>
        <tbody>

            {{#each persons }}
                        <tr>
                            <td>{{dataValues.id }}</td>
                            <td>{{ dataValues.full_name }}</td>
                            <td>{{ dataValues.dni }}</td>
                            <td>{{ dataValues.phone}}</td>
                            <td>{{ dataValues.email}}</td>
                            <td>{{ dataValues.address}}</td>
                            <td>{{ dataValues.instrument }}</td>
                        </tr>
            {{/each }}
        </tbody>

    </table>            